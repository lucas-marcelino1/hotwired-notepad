<%= turbo_stream.prepend "notes" do %>
  <% render @note %>
<% end %>

<%= turbo_stream.replace "new_note", partial: "button_partial" %>

<%= turbo_stream.update "message", inline: '<div class="bg-green-100 text-green-700 p-4 rounded-md mb-4 border border-green-300"><p>Note was successfully created</p></div>' %>